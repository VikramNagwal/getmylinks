generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model UserTable {
  id           Int           @id @default(autoincrement())
  username     String        @unique
  name         String
  email        String        @unique
  password     String
  isVerified   Boolean       @default(false)
  organization Organization? @default(INDIVIDUAL)
  userType     UserType      @default(Beta)
  refreshToken String?
}

enum Organization {
  INDIVIDUAL
  BUISNESS
}

enum UserType {
  Beta
  Premium
}
